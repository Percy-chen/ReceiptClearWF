{
	"_version": "1.1.0",
	"sap.app": {
		"id": "html5apps.receiptclearwf.ClearApproval",
		"_version": "1.3.0",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{APPTITLE}}",
		"description": "{{APPDESCRIPTION}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"API_CV_ATTACHMENT_SRV": {
				"uri": "/html5apps/receiptclearwf/destinations/WT_S4HC/API_CV_ATTACHMENT_SRV/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"API_DMS_PROCESS_SRV": {
				"uri": "/html5apps/receiptclearwf/destinations/WT_S4HC/API_DMS_PROCESS_SRV/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"WORKFLOWLOG.xsodata": {
				"uri": "/html5apps/receiptclearwf/destinations/APLEXHANA/WORKFLOWLOG.xsodata/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"CLEARRE": {
				"uri": "/html5apps/receiptclearwf/destinations/APLEXHANA/CLEARRE.xsodata/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"EMPLOYEES.xsodata": {
				"uri": "/html5apps/receiptclearwf/destinations/APLEXHANA/EMPLOYEES.xsodata/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			},
			"API_OPLACCTGDOCITEMCUBE_SRV": {
				"uri": "/html5apps/receiptclearwf/destinations/WT_S4HC/API_OPLACCTGDOCITEMCUBE_SRV/",
				"type": "OData",
				"settings": {
					"annotations": []
				}
			}
		}
	},

	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},

	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "ClearApproval.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ClearApproval.i18n.i18n"
				}
			},
			"Attach": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "API_CV_ATTACHMENT_SRV",
				"preload": true
			},
			"DIR": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "API_DMS_PROCESS_SRV",
				"preload": true
			},
			"WORKFLOWLOG": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "WORKFLOWLOG.xsodata",
				"preload": true
			},
			"currentUser": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "/html5apps/receiptclearwf/services/userapi/currentUser",
				"preload": false
			},
			"userAttributes": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false,
				"uri": "/html5apps/receiptclearwf/services/userapi/attributes"
			},
			"CLEARRE": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true,
					"defaultUpdateMethod": "Put"
				},
				"dataSource": "CLEARRE",
				"preload": true
			},
			"GetEMPLOYEES": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "EMPLOYEES.xsodata",
				"preload": true
			},
			"ACCTGDOC": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"useBatch": true
				},
				"dataSource": "API_OPLACCTGDOCITEMCUBE_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	},
	"sap.platform.hcp": {
		"uri": "ClearApproval/webapp",
		"_version": "1.1.0"
	}
}